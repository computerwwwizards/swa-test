{
  "routes": [
    {
      "route": "/my-page",
      "methods": ["GET"],
      "rewrite": "/api/render"
    },

    {
      "route": "/my-page/*",
      "methods": ["GET"],
      "rewrite": "/api/render"
    }
  ],

  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.map",
      "/*.png",
      "/*.jpg",
      "/*.svg",
      "/*.ico"
    ]
  }
}